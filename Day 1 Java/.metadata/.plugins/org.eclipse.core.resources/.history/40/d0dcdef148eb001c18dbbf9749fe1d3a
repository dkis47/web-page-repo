import java.io.*;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// Split string using space between words and place them into array []
		String sentence = "Are you as excited as I am?";
		String reverseSentence = "";
		String[] split = sentence.split("?");
		String[] split2 = sentence.split(" ");

		// for loop that iterate backwards

		for (int i = split2.length - 1; i >= 0; i--) {
			reverseSentence = reverseSentence + split2[i] + " ";

		}

		System.out.println(reverseSentence);

		// placing ? on the end of the sentence
		
		//if 
	}

}
